[program:la-metro-dashboard]
user=datamade
process_name=la-metro-dashboard
stdout_logfile=/var/log/la-metro-dashboard/webserver-out.log
stdout_logfile_maxbytes=10MB
stderr_logfile=/var/log/la-metro-dashboard/webserver-err.log
stderr_logfile_maxbytes=10MB
directory=/home/datamade/la-metro-dashboard
environment=
    AIRFLOW_HOME="/home/datamade/la-metro-dashboard",
    LA_METRO_DATABASE_URL="postgres://datamade@boardagendas.metro.net:5432/lametro_staging",
    LA_METRO_SOLR_URL="http://boardagendas.metro.net:8986/solr/lametro-staging",
    LA_METRO_DIR_PATH="/home/datamade/la-metro-councilmatic",
    SCRAPERS_DIR_PATH="/home/datamade/scrapers-us-municipal"
command=/bin/bash -c 'source "$0" && exec "$@"' /home/datamade/.virtualenvs/la-metro-dashboard/bin/activate airflow webserver -p 8080

[program:la-metro-dashboard-scheduler]
user=datamade
process_name=la-metro-dashboard-scheduler
stdout_logfile=/var/log/la-metro-dashboard/scheduler-out.log
stdout_logfile_maxbytes=10MB
stderr_logfile=/var/log/la-metro-dashboard/scheduler-err.log
stderr_logfile_maxbytes=10MB
directory=/home/datamade/la-metro-dashboard
environment=
    AIRFLOW_HOME="/home/datamade/la-metro-dashboard",
    LA_METRO_DATABASE_URL="postgres://datamade@boardagendas.metro.net:5432/lametro_staging",
    LA_METRO_SOLR_URL="http://boardagendas.metro.net:8986/solr/lametro-staging",
    LA_METRO_DIR_PATH="/home/datamade/la-metro-councilmatic",
    SCRAPERS_DIR_PATH="/home/datamade/scrapers-us-municipal"
command=/bin/bash -c 'source "$0" && exec "$@"' /home/datamade/.virtualenvs/la-metro-dashboard/bin/activate airflow scheduler
